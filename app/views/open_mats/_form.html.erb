<%= form_for @open_mat, :html => {:multipart => true} do |f| %>
  <%= render "shared/errors", object: @open_mat %>
  <p>
    <%= f.label :location %><br/>
    <%= f.text_field :location, autofocus: true %>
  </p>
  <p>
    <%= f.label :starts_at %>
    <%= f.datetime_local_field :starts_at %>
  </p>
  <p>
    <%= f.file_field :asset %>
  </p>
  <p>
    <%= f.submit %> |
    <%= link_to "Cancel", open_mats_path %>
  </p>
<% end %>